<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SwipeLayout"><attr name="drag_edge">
            <flag name="left" value="1"/>
            <flag name="right" value="2"/>
            <flag name="top" value="4"/>
            <flag name="bottom" value="8"/>
        </attr><attr format="dimension" name="leftEdgeSwipeOffset"/><attr format="dimension" name="rightEdgeSwipeOffset"/><attr format="dimension" name="topEdgeSwipeOffset"/><attr format="dimension" name="bottomEdgeSwipeOffset"/><attr format="enum" name="show_mode">
            <enum name="lay_down" value="0"/>
            <enum name="pull_out" value="1"/>
        </attr><attr format="boolean" name="clickToClose"/></declare-styleable>
    <string name="app_name">AndroidSwipeLayout</string>
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
</resources>